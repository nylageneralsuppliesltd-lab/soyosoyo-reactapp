{
  "name": "soyosoyo-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "prisma generate && prisma generate --config prisma.audit.config.ts && nest build",
    "start": "prisma migrate deploy && prisma migrate deploy --config prisma.audit.config.ts && node dist/main.js",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "start:dev": "dotenv -e .env -- nest start --watch",
    "prisma:generate:audit": "dotenv -e .env -- prisma generate --config prisma.audit.config.ts",
    "prisma:generate": "dotenv -e .env -- prisma generate",
    "prisma:db:pull": "dotenv -e .env -- prisma db pull",
    "prisma:studio": "dotenv -e .env -- prisma studio",
    "prisma:migrate:dev": "dotenv -e .env -- prisma migrate dev",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.9.5",
    "@nestjs/common": "^10.3.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/platform-express": "^10.3.0",
    "@prisma/adapter-neon": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "pg": "^8.17.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.0",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^10.2.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.10",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "dotenv": "^16.6.1",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3",
    "prisma": "^7.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.0"
  }
}
